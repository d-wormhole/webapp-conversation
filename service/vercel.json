{
  "headers": [
    {
      "source": "/api/(.*)",
      "destination": "http://agent.d-wormhole.com/v1"
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, max-age=0"
        },
        {
          "key": "Pragma",
          "value": "no-cache"
        }
      ]
    }
  ]
}
